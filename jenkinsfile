pipeline{
agent any

stages 
{
stage('Build') 
{
steps{
echo "Building the Code.........."
sh 'export MAVEN_HOME=/usr/local/bin/mvn'
sh 'mvn clean'
}
}
stage('Test') 
{
steps{
echo "Testing the Code.........."
echo $pwd
sh 'mvn test'
}
}
stage('Compile') 
{
steps{
echo "Compiling the Project.........."
sh 'mvn compile'
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}
